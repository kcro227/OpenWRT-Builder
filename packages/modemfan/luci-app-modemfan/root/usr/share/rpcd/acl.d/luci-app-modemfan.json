{
	"luci-app-modemfan": {
		"description": "Fan General Control",
		"read": {
			"uci": [
				"fancontrol"
			],
			"file": {
				"/sys/devices/platform/pwm-fan/hwmon/*/*": ["read"],
				"/usr/share/luci/applications/luci-app-modemfan/devices.json":["read"]
			}
		},
		"write": {
			"uci": [
				"fancontrol"
			]
		}
	}
}
